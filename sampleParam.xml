 <querysource>
  <name>docuv</name>
  <sql>SELECT a.fecha,a.serie,a.name,b.name as cli,a.total FROM co_docuv a INNER JOIN emp_socio b ON a.socio_id=b.id WHERE a.id>0
&lt;?if exists("sn")?>
AND a.serie= &lt;?value("ser")?> AND a.name= &lt;?value("num")?>
&lt;?endif?>
&lt;?if exists("ff")?>
AND a.fecha= &lt;?value("fec")?>
&lt;?endif?>
&lt;?if exists("cc")?>
AND b.name LIKE &lt;?value("cli")?>
&lt;?endif?>
LIMIT 30</sql>
